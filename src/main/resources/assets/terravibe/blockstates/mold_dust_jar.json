{
  "multipart": [
	{
	  "apply": {
		"model": "terravibe:block/jar"
	  }
	},
	{
	  "apply": {
		"model": "terravibe:block/cork_jar_plug"
	  },
	  "when": {
		"closed": "true"
	  }
	},
	{
	  "apply": {
		"model": "terravibe:block/jar_dust_height1"
	  },
	  "when": {
		"level": "1"
	  }
	},
	{
	  "apply": {
		"model": "terravibe:block/jar_dust_height2"
	  },
	  "when": {
		"level": "2"
	  }
	},
	{
	  "apply": {
		"model": "terravibe:block/jar_dust_height3"
	  },
	  "when": {
		"level": "3"
	  }
	}
  ]
}