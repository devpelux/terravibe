{
  "multipart": [
	{
	  "apply": {
		"model": "terravibe:block/jar"
	  }
	},
	{
	  "apply": {
		"model": "terravibe:block/cork_jar_plug"
	  },
	  "when": {
		"closed": "true"
	  }
	},
	{
	  "apply": {
		"model": "terravibe:block/jar_fluid_height1"
	  },
	  "when": {
		"AND": [ { "level": "1" }, { "content_texture": "fluid" } ]
	  }
	},
	{
	  "apply": {
		"model": "terravibe:block/jar_fluid_height2"
	  },
	  "when": {
		"AND": [ { "level": "2" }, { "content_texture": "fluid" } ]
	  }
	},
	{
	  "apply": {
		"model": "terravibe:block/jar_fluid_height3"
	  },
	  "when": {
		"AND": [ { "level": "3" }, { "content_texture": "fluid" } ]
	  }
	},
	{
	  "apply": {
		"model": "terravibe:block/jar_dense_fluid_height1"
	  },
	  "when": {
		"AND": [ { "level": "1" }, { "content_texture": "dense_fluid" } ]
	  }
	},
	{
	  "apply": {
		"model": "terravibe:block/jar_dense_fluid_height2"
	  },
	  "when": {
		"AND": [ { "level": "2" }, { "content_texture": "dense_fluid" } ]
	  }
	},
	{
	  "apply": {
		"model": "terravibe:block/jar_dense_fluid_height3"
	  },
	  "when": {
		"AND": [ { "level": "3" }, { "content_texture": "dense_fluid" } ]
	  }
	}
  ]
}