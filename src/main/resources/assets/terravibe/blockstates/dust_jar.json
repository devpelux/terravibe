{
  "multipart": [
	{
	  "apply": {
		"model": "terravibe:block/jar"
	  }
	},
	{
	  "apply": {
		"model": "terravibe:block/cork_jar_plug"
	  },
	  "when": {
		"closed": "true"
	  }
	},
	{
	  "apply": {
		"model": "terravibe:block/jar_burned_birch_mold_dust_height1"
	  },
	  "when": {
		"AND": [ { "level": "1" }, { "dust": "burned_birch_mold_dust" } ]
	  }
	},
	{
	  "apply": {
		"model": "terravibe:block/jar_burned_birch_mold_dust_height2"
	  },
	  "when": {
		"AND": [ { "level": "2" }, { "dust": "burned_birch_mold_dust" } ]
	  }
	},
	{
	  "apply": {
		"model": "terravibe:block/jar_burned_birch_mold_dust_height3"
	  },
	  "when": {
		"AND": [ { "level": "3" }, { "dust": "burned_birch_mold_dust" } ]
	  }
	},
	{
	  "apply": {
		"model": "terravibe:block/jar_burned_dark_mold_dust_height1"
	  },
	  "when": {
		"AND": [ { "level": "1" }, { "dust": "burned_dark_mold_dust" } ]
	  }
	},
	{
	  "apply": {
		"model": "terravibe:block/jar_burned_dark_mold_dust_height2"
	  },
	  "when": {
		"AND": [ { "level": "2" }, { "dust": "burned_dark_mold_dust" } ]
	  }
	},
	{
	  "apply": {
		"model": "terravibe:block/jar_burned_dark_mold_dust_height3"
	  },
	  "when": {
		"AND": [ { "level": "3" }, { "dust": "burned_dark_mold_dust" } ]
	  }
	},
	{
	  "apply": {
		"model": "terravibe:block/jar_burned_glowing_dark_mold_dust_height1"
	  },
	  "when": {
		"AND": [ { "level": "1" }, { "dust": "burned_glowing_dark_mold_dust" } ]
	  }
	},
	{
	  "apply": {
		"model": "terravibe:block/jar_burned_glowing_dark_mold_dust_height2"
	  },
	  "when": {
		"AND": [ { "level": "2" }, { "dust": "burned_glowing_dark_mold_dust" } ]
	  }
	},
	{
	  "apply": {
		"model": "terravibe:block/jar_burned_glowing_dark_mold_dust_height3"
	  },
	  "when": {
		"AND": [ { "level": "3" }, { "dust": "burned_glowing_dark_mold_dust" } ]
	  }
	}
  ]
}